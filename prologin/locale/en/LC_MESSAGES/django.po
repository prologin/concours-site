#: prologin/templates/prologin/main-footer.html:8
#: team/templates/team/index.html:7
msgid "The organization"
msgstr ""

#: pages/templates/pages/about-organization.html:10
#: pages/templates/pages/about-organization.html:15
msgid "About the Prologin organization"
msgstr ""

#: pages/templates/pages/about-organization.html:17
#, python-format
msgid ""
"Prologin is a nonprofit organization whose main mission is to develop and "
"organize the\n"
"<a href=\"%(contest_url)s\">Prologin computing contest</a>."
msgstr ""

